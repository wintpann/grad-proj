<h3 class="page-name">Deleted products</h3>
<% if @products && @products.any? %>
  <ul>
    <% @products.each do |product| %>
      <li class="collection-item list-3">
        <%= link_to product.code, product_path(product) %>
        <%= content_tag :span, "(#{product.name})" %>
      </li>
    <% end %>
  </ul>
<% end %>
