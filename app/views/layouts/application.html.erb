<!DOCTYPE html>
<html>
  <head>
    <title>Склад</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <%= render 'shared/header' %>
    <div class="wrapper">
      <%= render 'shared/flash' %>
      <%= render 'shared/errors' %>
      <div class="content">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
