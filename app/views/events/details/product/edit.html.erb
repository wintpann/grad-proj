<%= content_tag :h3, "Product updated | #{to_local_time(@event.created_at)} | by #{@event.editor.name} #{@event.editor.lastname} | (#{@event.product_change_event.head_event.id})", class: 'page-name'%>
<div class="event_details">
  <div class="half"><%= render 'snap/product', snap: @event.snap_from %></div>
  <div class="half"><%= render 'snap/product', snap: @event.snap_to %></div>
</div>
