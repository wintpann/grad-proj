<ul class="event_info">
  <li><%= content_tag :span, "Пользователь восстановлен (#{event.user_change_event.head_event.id})"%></li>
  <li><%= link_to event.user_snap.now.identifier, user_url(event.user_snap.user_id), class: :mark %></li>
  <li><%= content_tag :span, to_local_time(event.created_at)%></li>
  <li><%= link_to event.editor.identifier, user_url(event.editor.id), class: :mark %></li>
  <li><%= link_to 'Подробно', event_details_url(id: event.user_change_event.head_event.id), class: :btn2, target: :_blank  %></li>
  <li><%= link_to 'Печать', event_details_url(id: event.user_change_event.head_event.id, format: 'pdf'), class: 'btn2 hid', target: :_blank  %></li>
</ul>
