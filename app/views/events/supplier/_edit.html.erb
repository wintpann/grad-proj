<ul class="event_info">
  <li><%= content_tag :span, "Supplier updated (#{event.supplier_change_event.head_event.id})"%></li>
  <li><%= link_to event.snap_from.now.name, supplier_url(event.snap_from.supplier_id), class: :mark %></li>
  <li><%= content_tag :span, to_local_time(event.created_at)%></li>
  <li><%= link_to event.editor.identifier, user_url(event.editor.id), class: :mark %></li>
  <li><%= link_to 'Details', event_details_url(id: event.supplier_change_event.head_event.id), class: :btn2, target: :_blank  %></li>
  <li><%= link_to 'Print', event_details_url(id: event.supplier_change_event.head_event.id, format: 'pdf'), class: 'btn2 hid', target: :_blank  %></li>
</ul>
